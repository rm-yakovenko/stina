{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"airbnb",
			"prettier"
	],
	"parserOptions": {
			"ecmaFeatures": {
					"jsx": true
			},
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"plugins": [
			"react"
	],
	"rules": {
		"no-unused-vars":"warn",
		"import/no-unresolved":"off",
		"import/prefer-default-export": "off",
		"spaced-comment":"warn"
	}
}
